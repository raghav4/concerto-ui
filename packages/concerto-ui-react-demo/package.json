{
  "name": "@accordproject/concerto-ui-react-demo",
  "version": "0.82.9",
  "description": "Form generator for Accord Project Concerto modeling language type",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "private": true,
  "dependencies": {
    "@accordproject/concerto-ui-react": "0.82.9",
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-transform-modules-commonjs": "^7.7.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@clausehq/eslint-config": "^0.1.8",
    "@testing-library/dom": "^6.10.1",
    "@testing-library/react": "^9.3.2",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "braces": "3.0.2",
    "chance": "^1.1.3",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^4.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.13.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
		"react-dom": "^16.11.0",
    "sass-loader": "^7.1.0",
    "semver": "6.1.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.87.2",
    "style-loader": "^0.23.1",
    "styled-components": "^4.4.1",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "scripts": {
    "build": "webpack --config webpack.config.prod.js --mode production",
    "start": "webpack-dev-server --config webpack.config.js --mode development --watch ",
    "dev": "webpack-dev-server --mode development --config webpack.config.js",
    "pretest": "npm run lint",
    "lint": "eslint .",
    "postlint": "npm run licchk",
    "licchk": "license-check-and-add",
    "postlicchk": "npm run doc",
    "doc": "jsdoc --pedantic --recurse -c jsdoc.json"
  },
  "keywords": [
    "blockchain",
    "hyperledger",
    "solutions"
  ],
  "author": "Hyperledger Composer",
  "license": "Apache-2.0",
  "devDependencies": {
    "jsdoc": "3.5.5",
    "license-check-and-add": "2.3.6"
  },
  "license-check-and-add-config": {
    "folder": ".",
    "license": "LICENSE.txt",
    "exact_paths_method": "EXCLUDE",
    "exact_paths": [
      "coverage",
      "LICENSE.txt",
      "node_modules",
      ".nyc-output",
      "out",
      ".git",
      "yarn.lock",
      "yarn-error.log",
      "README.md",
      "src/registerServiceWorker.js",
      "src/components/FormBuilder",
      "public",
      "config",
      ".babelrc",
      "dist"
    ],
    "file_type_method": "EXCLUDE",
    "file_types": [
      ".yml",
      ".yaml",
      ".zip",
      ".tgz"
    ],
    "insert_license": false,
    "license_formats": {
      "js|njk|pegjs|cto|acl|qry": {
        "prepend": "/*",
        "append": " */",
        "eachLine": {
          "prepend": " * "
        }
      },
      "npmrc|editorconfig|txt": {
        "eachLine": {
          "prepend": "# "
        }
      }
    }
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "lib/codegen/javascriptparser.js",
      "lib/codegen/parsejs.js",
      "lib/introspect/parser.js",
      "lib/acl/parser.js",
      "lib/query/parser.js",
      "out/**",
      "scripts/**",
      "systest/**",
      "test/**"
    ],
    "reporter": [
      "text-summary",
      "html",
      "cobertura",
      "lcov",
      "clover"
    ],
    "include": [
      "**/.js"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
